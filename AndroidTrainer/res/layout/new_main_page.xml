<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" 	
	android:layout_height="match_parent" 
	android:layout_width="match_parent" 
	android:orientation="vertical"
	android:layout_margin="0dip"
	android:id="@+id/objMainLayout" 	
	android:background="@drawable/main_gradient" 
	android:clickable="true" android:gravity="right">	
	<RelativeLayout android:padding="6dip" android:background="@drawable/lista_title" android:layout_width="fill_parent" android:id="@+id/relativeLayout1" android:layout_height="wrap_content">
            <LinearLayout android:layout_width="match_parent" android:id="@+id/linearLayout1" android:layout_height="wrap_content"></LinearLayout>
            <TextView android:text="@string/app_name_buy" android:textColor="#FFFFFFFF" android:id="@+id/text_title" android:textSize="14sp" android:layout_centerHorizontal="true" android:layout_width="wrap_content" android:textStyle="bold" android:layout_centerVertical="true" android:layout_height="wrap_content"></TextView>
        </RelativeLayout>
	<ScrollView 
        	android:id="@+id/scrollView1" 
        	android:layout_width="match_parent" 
        	android:layout_height="match_parent">
			<LinearLayout 
				android:id="@+id/internal" 
				android:orientation="vertical" 
				android:layout_height="match_parent" 
				android:layout_width="match_parent" 
				
				android:padding="5dip">
				
			    <LinearLayout 
						android:layout_height="wrap_content" 
						android:layout_width="match_parent" 
						android:background="@drawable/lista_gradient"
						android:padding="10dip"
						android:layout_marginTop="10dip" 
						android:orientation="vertical">
						<RelativeLayout 
							android:layout_height="match_parent" 
							android:layout_width="match_parent" 
							android:background="@drawable/lista_gradient"
							android:padding="10dip"
							android:layout_marginTop="0dip" 
							android:orientation="horizontal" 
							android:clickable="true"
						    android:id="@+id/btn_bmi">
						
							<TextView
					            android:id="@+id/txtBMI"
					            android:layout_width="wrap_content"
					            android:layout_height="wrap_content"	           
					            android:text="@string/bmi"
					            android:textColor="#FFFFFF"
					            android:textSize="20sp" />
							<TextView
							    android:id="@+id/txtBMIGO"
							    android:layout_width="wrap_content"
							    android:layout_height="wrap_content"
							    android:layout_alignParentRight="true"
							    android:text=">>"
							    android:textColor="#FFFFFF"
							    android:textSize="20sp"
							    android:visibility="visible" />

							<TextView
							    android:id="@+id/txtBMIValue"
							    android:layout_width="wrap_content"
							    android:layout_height="wrap_content"
							    android:layout_alignBaseline="@+id/txtBMI"
							    android:layout_alignBottom="@+id/txtBMI"
							    android:layout_marginLeft="20dp"
							    android:layout_toRightOf="@+id/txtBMI"
							    android:text=" - "
							    android:textColor="#FFFFFF"
							    android:textSize="18sp" />

						</RelativeLayout>		      
				</LinearLayout>
			    
				<!-- Run Workout -->
				<LinearLayout 
						android:layout_height="wrap_content" 
						android:layout_width="match_parent" 
						android:background="@drawable/lista_gradient"
						android:padding="10dip"
						android:layout_marginTop="10dip" 
						android:orientation="vertical">
						<LinearLayout 
							android:layout_height="match_parent" 
							android:layout_width="match_parent" 
							android:background="@drawable/lista_gradient"
							android:padding="10dip"
							android:layout_marginTop="0dip" 
							android:orientation="horizontal" 
							android:clickable="true"
						    android:id="@+id/btn_start_running">
							<ImageView 
								android:layout_height="wrap_content" 
								android:layout_width="wrap_content" 								
								android:id="@+id/imgTypeExercise" 
								android:src="@drawable/running" 
								android:layout_marginTop="0dip"></ImageView>
							<LinearLayout 
								android:layout_height="match_parent" 
								android:layout_width="match_parent" 
								android:padding="0dip"
								android:layout_marginTop="0dip" 
								android:orientation="vertical">		
								<Button 
				                	android:clickable="true" 
				                	android:layout_width="wrap_content" 
				                	android:textSize="16sp" 
				                	android:layout_height="wrap_content" 
				                	android:background="@drawable/lista_gradient" 
				                	android:id="@+id/btnManualRun"                	
				                	android:text="@string/manual"
				                	android:textColor="#FFFFFF" android:layout_gravity="right"></Button>						
								<TextView 
									android:layout_height="match_parent" 
									android:layout_width="match_parent" 
									android:id="@+id/textNote" 
									android:text="@string/startexercise_dett1" 
									android:textColor="#FFFFFF" 
									android:textSize="10sp"
									android:layout_marginLeft="20dip" 
									android:gravity="bottom|right"></TextView>
								</LinearLayout>
						</LinearLayout>
				</LinearLayout>
				<!-- Run Workout -->
				
				<!-- Walk Workout -->
				<LinearLayout 
						android:layout_height="wrap_content" 
						android:layout_width="match_parent" 
						android:background="@drawable/lista_gradient"
						android:padding="10dip"
						android:layout_marginTop="10dip" 
						android:orientation="vertical" >
						<LinearLayout 
							android:layout_height="match_parent" 
							android:layout_width="match_parent" 
							android:background="@drawable/lista_gradient"
							android:padding="10dip"
							android:layout_marginTop="0dip" 
							android:orientation="horizontal"
							android:clickable="true"
						    android:id="@+id/btn_start_walking">
							<ImageView 
								android:layout_height="wrap_content" 
								android:layout_width="wrap_content" 								
								android:id="@+id/imgTypeExercise" 
								android:src="@drawable/walking" 
								android:layout_marginTop="0dip"></ImageView>
							<LinearLayout 
								android:layout_height="match_parent" 
								android:layout_width="match_parent" 
								android:padding="0dip"
								android:layout_marginTop="0dip" 
								android:orientation="vertical">	
								<Button 
				                	android:clickable="true" 
				                	android:layout_width="wrap_content" 
				                	android:textSize="16sp" 
				                	android:layout_height="wrap_content" 
				                	android:background="@drawable/lista_gradient" 
				                	android:id="@+id/btnManualWalk"                	
				                	android:text="@string/manual"
				                	android:textColor="#FFFFFF" android:layout_gravity="right"></Button>							
								<TextView 
									android:layout_height="match_parent" 
									android:layout_width="match_parent" 
									android:id="@+id/textNote" 
									android:text="@string/startexercise_dett1" 
									android:textColor="#FFFFFF" 
									android:textSize="10sp"
									android:layout_marginLeft="20dip" 
									android:gravity="bottom|right"></TextView>
								</LinearLayout>
						</LinearLayout>
				</LinearLayout>
				<!-- Walk Workout -->
				
				<!-- Bike Workout -->
				<LinearLayout 
						android:layout_height="wrap_content" 
						android:layout_width="match_parent" 
						android:background="@drawable/lista_gradient"
						android:padding="10dip"
						android:layout_marginTop="10dip" 
						android:orientation="vertical">
						<LinearLayout 
							android:layout_height="match_parent" 
							android:layout_width="match_parent" 
							android:background="@drawable/lista_gradient"
							android:padding="10dip"
							android:layout_marginTop="0dip" 
							android:orientation="horizontal"
							android:clickable="true"
						    android:id="@+id/btn_start_biking">
							<ImageView 
								android:layout_height="wrap_content" 
								android:layout_width="wrap_content" 								
								android:id="@+id/imgTypeExercise" 
								android:src="@drawable/biking" 
								android:layout_marginTop="0dip"></ImageView>
							<LinearLayout 
								android:layout_height="match_parent" 
								android:layout_width="match_parent" 
								android:padding="0dip"
								android:layout_marginTop="0dip" 
								android:orientation="vertical">		
								<Button 
				                	android:clickable="true" 
				                	android:layout_width="wrap_content" 
				                	android:textSize="16sp" 
				                	android:layout_height="wrap_content" 
				                	android:background="@drawable/lista_gradient" 
				                	android:id="@+id/btnManualBike"                	
				                	android:text="@string/manual"
				                	android:textColor="#FFFFFF" android:layout_gravity="right"></Button>						
								<TextView 
									android:layout_height="match_parent" 
									android:layout_width="match_parent" 
									android:id="@+id/textNote" 
									android:text="@string/startexercise_dett1" 
									android:textColor="#FFFFFF" 
									android:textSize="10sp"
									android:layout_marginLeft="20dip" 
									android:gravity="bottom|right"></TextView>
								</LinearLayout>
						</LinearLayout>
				</LinearLayout>
				<!-- Bike Workout -->
				
				<!-- Function Trainer -->																						
				<LinearLayout 
					android:orientation="vertical" 
					android:layout_marginTop="25dip" 
					android:layout_width="match_parent" 
					android:id="@+id/LinearLayout01" 
					android:layout_height="wrap_content"
					android:padding="20dip"
					android:background="@drawable/lista_gradient">
					<Button android:layout_height="wrap_content"
						android:layout_width="match_parent" 
						android:text="@string/tabHistory_label" 
						android:textSize="25sp" 
						android:background="@drawable/lista_gradient"
						android:textColor="#FFFFFF" 
						android:id="@+id/btn_history" android:clickable="true"></Button>
					
					<Button android:layout_height="wrap_content" 
						android:layout_marginTop="20dip" android:layout_width="match_parent" 
						android:text="@string/weight" 
						android:textSize="25sp" 
						android:background="@drawable/lista_gradient"
						android:textColor="#FFFFFF" 
						android:id="@+id/btn_weight" android:clickable="true"></Button>
					<Button 
						android:layout_height="wrap_content" 
						android:layout_marginTop="20dip" 
						android:layout_width="match_parent" 
						android:text="@string/tabPref_label" 
						android:textSize="25sp" 
						android:background="@drawable/lista_gradient"
						android:textColor="#FFFFFF" 
						android:id="@+id/btn_settings" android:clickable="true"></Button>
					
					<Button android:layout_height="wrap_content" 
					    android:layout_marginTop="20dip" 
						android:layout_width="match_parent" 
						android:text="@string/tabAbout_label" 
						android:textSize="25sp" 
						android:background="@drawable/lista_gradient"
						android:textColor="#FFFFFF" 
						android:id="@+id/btn_info" android:clickable="true"></Button>
					
					<Button android:layout_height="wrap_content"
						android:layout_marginTop="20dip"
						android:layout_width="match_parent" 
						android:text="@string/tabStore_label" 
						android:textSize="25sp" 
						android:background="@drawable/lista_gradient"
						android:textColor="#FFFFFF" 
						android:id="@+id/btn_trainer_store" android:clickable="true"></Button>
					    																	
				</LinearLayout>
				<!-- Function Trainer -->	
				
				<View 
					android:layout_weight="1" 
					android:layout_marginRight="0dip" 
					android:layout_width="fill_parent" 
					
					android:id="@+id/view3" 
					android:layout_marginBottom="0dip" 
					android:layout_height="1dip" 
					android:layout_marginTop="2dip" 
					android:layout_marginLeft="0dip"
					android:background="@drawable/lista_gradient">
				</View>				
			</LinearLayout>		
		</ScrollView>
</LinearLayout>